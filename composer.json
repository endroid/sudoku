{
    "name": "endroid/sudoku",
    "description": "Endroid Sudoku solver",
    "keywords": ["endroid", "sudoku", "solver", "php"],
    "homepage": "https://github.com/endroid/sudoku",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "endroid/quality": "dev-main"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Sudoku\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Sudoku\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
